create view sysMLAndUML as

select trace.Log join sysMl.Block as blockComponent,

       
from 'http://www.eclipse.org/papyrus/sysml/1.6/SysML/Blocks' as sysMl,
     'http://trace' as trace
     
where s.name = t.name and t.instanceClassName = 'org.eclipse.papyrus.sysml6.portsandflows.InterfaceBlock' for blockComponent